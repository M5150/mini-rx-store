{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "mini-rx-store": [
        "dist/mini-rx-store"
      ],
      "mini-rx-store/*": [
        "dist/mini-rx-store/*"
      ],
      "mini-rx-ng-devtools": [
        "dist/mini-rx-ng-devtools"
      ],
      "mini-rx-ng-devtools/*": [
        "dist/mini-rx-ng-devtools/*"
      ]
    },
      "esModuleInterop": true
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
