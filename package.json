{
    "name": "mini-rx-store",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "test:minirx": "jest --config ./projects/mini-rx-store/jest.config.js",
        "pretty-quick": "pretty-quick",
        "minirx-version++": "cd projects/mini-rx-store && npm version prerelease",
        "minirx-build": "npm run minirx-version++ && ng build mini-rx-store && copy LICENSE dist/mini-rx-store && copy README.md dist/mini-rx-store",
        "showcase-analyze": "ng build  mini-rx-store-showcase --outputPath=dist --prod --stats-json && webpack-bundle-analyzer dist/stats-es2015.json --port 4201"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.13",
        "@angular/common": "~8.2.13",
        "@angular/compiler": "~8.2.13",
        "@angular/core": "~8.2.13",
        "@angular/forms": "~8.2.13",
        "@angular/platform-browser": "~8.2.13",
        "@angular/platform-browser-dynamic": "~8.2.13",
        "@angular/router": "~8.2.13",
        "angular-in-memory-web-api": "^0.9.0",
        "bootstrap": "^4.4.1",
        "memoize-one": "^5.1.1",
        "rxjs": "~6.4.0",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.23",
        "@angular-devkit/build-ng-packagr": "~0.803.23",
        "@angular/cli": "~8.3.18",
        "@angular/compiler-cli": "~8.2.13",
        "@angular/language-service": "~8.2.13",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "^25.2.1",
        "@types/node": "~8.9.4",
        "codelyzer": "^5.0.0",
        "copy": "^0.3.2",
        "husky": "^4.2.3",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^25.3.0",
        "jest-preset-angular": "^8.1.3",
        "ng-packagr": "^5.4.0",
        "prettier": "2.0.2",
        "pretty-quick": "^2.0.1",
        "protractor": "~5.4.0",
        "ts-jest": "^25.3.1",
        "ts-node": "~7.0.0",
        "tsickle": "^0.37.0",
        "tslint": "~5.15.0",
        "typescript": "~3.5.3",
        "webpack-bundle-analyzer": "^3.6.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
